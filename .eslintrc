{
    "extends": "eslint:recommended",
    "rules": {
      "no-empty": [
        2,
        {
          "allowEmptyCatch": true
        }
      ],
      "curly": 2,
      "no-eq-null": 2,
      "no-eval": 2,
      "no-extend-native": 2,
      "no-implied-eval": 2,
      "global-require": 2,
      "handle-callback-err": 2,
      "no-mixed-requires": 2,
      "no-new-require": 2,
      "no-path-concat": 2,
      "no-process-exit": 2,
      "no-restricted-modules": [
        2,
        "domian"
      ],
      "no-sync": 2,
      "block-spacing": [
        2,
        "always"
      ],
      "max-len": [
        2,
        120
      ],
      "quotes": [
        2,
        "single"
      ],
      "guard-for-in": 2,
      "keyword-spacing": 2,
      "space-infix-ops": 2
    },
    "env": {
      "node": true,
      "mocha": true,
      "es6": true
    }
  }
  